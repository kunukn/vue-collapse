{
  "name": "@kunukn/vue-collapse",
  "version": "0.0.5",
  "author": "kunukn",
  "license": "MIT",
  "description": "Collapsible library based on CSS transition for Vue",
  "type": "module",
  "files": [
    "dist/*"
  ],
  "main": "./dist/vue-collapse.umd.cjs",
  "module": "./dist/vue-collapse.js",
  "exports": {
    ".": {
      "import": "./dist/vue-collapse.js",
      "require": "./dist/vue-collapse.umd.cjs"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kunukn/vue-collapse.git"
  },
  "keywords": [
    "vue",
    "collapse",
    "collapsible",
    "accordion",
    "CSS transition",
    "library"
  ],
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc && vite build",
    "build-only": "vite build",
    "type-check": "vue-tsc --noEmit -p tsconfig.vitest.json --composite false",
    "preview": "vite preview",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "test": "vitest --environment jsdom --root src/",
    "coverage": "vitest run --coverage"
  },
  "dependencies": {},
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.1.4",
    "@types/node": "^18.11.18",
    "@types/jsdom": "^20.0.1",
    "@vitejs/plugin-vue": "^4.0.0",
    "@vue/eslint-config-prettier": "^7.0.0",
    "@vue/eslint-config-typescript": "^11.0.0",
    "@vitest/coverage-c8": "^0.28.5",
    "@vue/test-utils": "^2.2.6",
    "@vue/tsconfig": "^0.1.3",
    "jsdom": "^21.1.0",
    "npm-run-all": "^4.1.5",
    "typescript": "^4.9.3",
    "vite": "^4.0.0",
    "vitest": "^0.28.5",
    "vue": "^3.2.45",
    "vue-tsc": "^1.0.11",
    "eslint": "^8.22.0",
    "eslint-plugin-vue": "^9.3.0",
    "prettier": "^2.7.1"
  },
  "peerDependencies": {
    "vue": "3.x.x"
  }
}
